<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="375.0" prefWidth="558.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="CarApplication.User.Controllers.RegisterPageController">

   <children>
      <Label layoutX="200.0" layoutY="14.0" text="Register Form">
         <font>
            <Font name="Bell MT" size="26.0" />
         </font>
      </Label>
      <TextField fx:id="registerUsername" layoutX="148.0" layoutY="153.0" prefHeight="31.0" prefWidth="262.0" promptText="Username" />
      <PasswordField fx:id="registerPassword" layoutX="148.0" layoutY="192.0" prefHeight="31.0" prefWidth="262.0" promptText="Password" />
      <Button fx:id="registerPageRegisterBTN" layoutX="246.0" layoutY="273.0" mnemonicParsing="false" prefWidth="63.0" text="Register" onAction="#register" />
      <Label layoutX="240.0" layoutY="308.0" text="Login Account" />
      <Button fx:id="registerLoginBTN" layoutX="232.0" layoutY="335.0" mnemonicParsing="false" onAction="#functionRegisterLoginBTN" text="Login Account" />
      <TextField fx:id="registerFirstName" layoutX="148.0" layoutY="71.0" prefHeight="31.0" prefWidth="262.0" promptText="First Name" />
      <TextField fx:id="registerLastName" layoutX="148.0" layoutY="111.0" prefHeight="31.0" prefWidth="262.0" promptText="Last Name" />
      <PasswordField fx:id="registerConfirmPassword" layoutX="147.0" layoutY="235.0" prefHeight="31.0" prefWidth="262.0" promptText="Confirm Password" />
   </children>
</AnchorPane>