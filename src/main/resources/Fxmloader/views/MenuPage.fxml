<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/20.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="CarApplication.User.Controllers.MenuPageController">
   <children>
      <AnchorPane layoutX="14.0" layoutY="14.0" prefHeight="375.0" prefWidth="558.0">
         <children>
            <Label layoutX="177.0" layoutY="39.0" text="Sign in Account">
               <font>
                  <Font name="Bell MT" size="26.0" />
               </font>
            </Label>
            <TextField fx:id="loginUsername" layoutX="127.0" layoutY="100.0" prefHeight="31.0" prefWidth="262.0" promptText="Username" />
            <PasswordField fx:id="loginPassword1" layoutX="126.0" layoutY="144.0" prefHeight="31.0" prefWidth="262.0" promptText="Password" />
            <TextField fx:id="loginPassword2" layoutX="126.0" layoutY="144.0" prefHeight="31.0" prefWidth="262.0" promptText="Password" visible="false"/>
            <CheckBox fx:id="showPassword" layoutX="127.0" layoutY="185.0" mnemonicParsing="false" onAction="#functionShowPassword" prefHeight="16.0" prefWidth="151.0" text="Show Password" />
            <Button fx:id="loginPageBTN" layoutX="226.0" layoutY="222.0" mnemonicParsing="false" onAction="#functionLoginBTN" prefWidth="63.0" text="Login" />
            <Label layoutX="217.0" layoutY="270.0" text="Register Account" />
            <Button fx:id="loginRegisterProceedBTN" layoutX="231.0" layoutY="295.0" mnemonicParsing="false" onAction="#functionRegisterBTN" text="Register" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
